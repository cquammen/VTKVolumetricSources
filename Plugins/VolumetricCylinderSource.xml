<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <SourceProxy name="VolumetricCylinderSource" class="vtkVolumetricCylinderSource" label="Volumetric Cylinder Source">
      <Documentation
         long_help=""
         short_help="">
      </Documentation>

      <DoubleVectorProperty
         name="Height"
         command="SetHeight"
         number_of_elements="1"
         default_values="0.1">
        <DoubleRangeDomain name="range"/>
      </DoubleVectorProperty>

      <DoubleVectorProperty
         name="Radius"
         command="SetRadius"
         number_of_elements="1"
         default_values="0.1">
        <DoubleRangeDomain name="range"/>
      </DoubleVectorProperty>
      
      <IntVectorProperty
         name="Resolution"
         command="SetResolution"
         number_of_elements="1"
         default_values="16">
        <IntRangeDomain name="range" min="3"/>
      </IntVectorProperty>

    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
